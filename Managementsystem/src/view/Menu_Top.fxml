<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="60.0" maxWidth="1055.0" minHeight="60.0" minWidth="1055.0" prefHeight="60.0" prefWidth="1055.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Menu_Top_Controller">
   <children>
      <HBox fx:id="toppane" alignment="CENTER_LEFT" minHeight="50.0" prefWidth="1055.0" style="-fx-background-color: #000000;">
         <children>
            <ImageView fitHeight="60.0" fitWidth="120.0">
               <image>
                  <Image url="@../images/LOGOV3T%20changed.png" />
               </image>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </ImageView>
            <Label text="Ace Group" textFill="WHITE">
               <font>
                  <Font name="System Italic" size="36.0" />
               </font>
            </Label>
            <ImageView fitHeight="50.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <HBox.margin>
                  <Insets right="-150.0" />
               </HBox.margin>
            </ImageView>
            <ImageView fitHeight="24.0" fitWidth="24.0">
               <image>
                  <Image url="@../images/homePage_symbol.png" />
               </image>
            </ImageView>
            <Button fx:id="homepagebutton" mnemonicParsing="false" onAction="#switchToHomePage" style="-fx-background-color: black;" text="Home Page" textFill="WHITE">
               <font>
                  <Font name="Arial Bold" size="16.0" />
               </font>
            </Button>
            <ImageView fitHeight="23.0" fitWidth="23.0">
               <image>
                  <Image url="@../images/contact_symbol.png" />
               </image>
            </ImageView>
            <Button fx:id="contactbutton" mnemonicParsing="false" onAction="#switchToContactPage" style="-fx-background-color: black;" text="Contact" textFill="WHITE">
               <font>
                  <Font name="Arial Bold" size="16.0" />
               </font>
            </Button>
            <ImageView fitHeight="20.0" fitWidth="34.0">
               <image>
                  <Image url="@../images/teamMember_symbol.png" />
               </image>
            </ImageView>
            <Button fx:id="teammembersbutton" mnemonicParsing="false" onAction="#switchToMemberPage" style="-fx-background-color: black;" text="Team Members" textFill="WHITE">
               <font>
                  <Font name="Arial Bold" size="16.0" />
               </font>
            </Button>
            <ImageView fitHeight="27.0" fitWidth="27.0">
               <image>
                  <Image url="@../images/accountSettings_symbol.png" />
               </image>
            </ImageView>
            <Button fx:id="accountsettingsbutton" mnemonicParsing="false" onAction="#switchToSettingsPage" style="-fx-background-color: black;" text="Account Settings" textFill="WHITE">
               <font>
                  <Font name="Arial Bold" size="16.0" />
               </font>
            </Button>
            <ImageView fitHeight="29.0" fitWidth="29.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/login_button.png" />
               </image>
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </ImageView>
            <Button fx:id="loginbutton" mnemonicParsing="false" onAction="#switchToLoginPage" style="-fx-background-color: black;" text="Log in" textFill="WHITE">
               <font>
                  <Font name="Arial Bold" size="16.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
